{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@pub/*": ["public/*"],
      "@utils/*": ["src/utils/*"],
      "@styles/*": ["src/styles/*"],
      "@layouts/*": ["src/layouts/*"],
      "@data/*": ["src/data/*"],
      "@components/*": ["src/components/*"],
      "@common-components/*": ["src/components/common/*"],
      "@interfaces/*": ["src/interfaces/*"],
      "@icons/*": ["src/icons/*"]
    }
  }
}
