{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@pub/*": ["public/*"],
      "@components/*": ["src/components/*"],
      "@common-components/*": ["src/components/common/*"],
      "@interfaces/*": ["src/interfaces/*"],
      "@icons/*": ["src/icons/*"]
    }
  }
}
