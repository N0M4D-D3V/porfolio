---
import Section from "@common-components/Section.astro";

const { history } = Astro.props;
---

<Section title={history.title}>
  {
    history.paragraphs?.map((text: string) => {
      return <p>{text}</p>;
    })
  }
</Section>

<style>
  p {
    margin-bottom: 2%;
    padding-left: 2%;
  }
</style>
